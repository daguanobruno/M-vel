<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.usuario.trabalhojoaopaulo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PrincipalActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
    </activity>

        <activity
            android:name=".GastosActivity"
            android:parentActivityName=".PrincipalActivity" />

        <activity
            android:name=".InformacoesActivity"
            android:parentActivityName=".PrincipalActivity" />

        <activity
            android:name=".LocalActivity"
            android:parentActivityName=".LocaisActivity" />

        <activity
            android:name=".LocaisActivity"
            android:parentActivityName=".LocalActivity" />

        <activity
            android:name=".SharedPreferencesActivity"
            android:parentActivityName=".PrincipalActivity" />

    </application>

</manifest>